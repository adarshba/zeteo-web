{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "framework": "sveltekit",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ],
  "functions": {
    "api/*.ts": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "env": {
    "AI_PROVIDER": "@ai_provider",
    "OPENAI_API_KEY": "@openai_api_key",
    "AI_API_KEY": "@ai_api_key",
    "AI_MODEL": "@ai_model"
  }
}
